{
  "name": "world-monitor",
  "private": true,
  "version": "2.5.8",
  "license": "AGPL-3.0-only",
  "type": "module",
  "scripts": {
    "lint:md": "markdownlint-cli2 '**/*.md' '!.agent/**' '!.agents/**' '!.claude/**' '!.factory/**' '!.windsurf/**' '!skills/**' '!docs/internal/**' '!docs/Docs_To_Review/**'",
    "version:sync": "node scripts/sync-desktop-version.mjs",
    "version:check": "node scripts/sync-desktop-version.mjs --check",
    "dev": "cross-env VITE_VARIANT=finance vite",
    "dev:finance": "cross-env VITE_VARIANT=finance vite",
    "dev:tesserect": "cross-env VITE_VARIANT=tesserect vite",
    "build": "tsc && vite build",
    "build:sidecar-sebuf": "node scripts/build-sidecar-sebuf.mjs",
    "build:desktop": "node scripts/build-sidecar-sebuf.mjs && tsc && vite build",
    "build:finance": "cross-env VITE_VARIANT=finance tsc && cross-env VITE_VARIANT=finance vite build",
    "build:tesserect": "cross-env VITE_VARIANT=tesserect tsc && cross-env VITE_VARIANT=tesserect vite build",
    "typecheck": "tsc --noEmit",
    "tauri": "tauri",
    "preview": "vite preview",
    "test:e2e:finance": "VITE_VARIANT=finance playwright test",
    "test:e2e:tesserect": "VITE_VARIANT=tesserect playwright test",
    "test:e2e:runtime": "VITE_VARIANT=finance playwright test e2e/runtime-fetch.spec.ts",
    "test:e2e": "npm run test:e2e:runtime && npm run test:e2e:finance",
    "test:data": "node --test tests/*.test.mjs",
    "test:sidecar": "node --test src-tauri/sidecar/local-api-server.test.mjs api/_cors.test.mjs api/youtube/embed.test.mjs api/cyber-threats.test.mjs api/usni-fleet.test.mjs scripts/ais-relay-rss.test.cjs api/loaders-xml-wms-regression.test.mjs",
    "test:e2e:visual:finance": "VITE_VARIANT=finance playwright test -g \"matches golden screenshots per layer and zoom\"",
    "test:e2e:visual": "npm run test:e2e:visual:finance",
    "test:e2e:visual:update:finance": "VITE_VARIANT=finance playwright test -g \"matches golden screenshots per layer and zoom\" --update-snapshots",
    "test:e2e:visual:update": "npm run test:e2e:visual:update:finance",
    "desktop:dev": "npm run version:sync && VITE_DESKTOP_RUNTIME=1 tauri dev -f devtools",
    "desktop:build:finance": "npm run version:sync && VITE_VARIANT=finance VITE_DESKTOP_RUNTIME=1 tauri build --config src-tauri/tauri.finance.conf.json",
    "desktop:package:macos:finance": "node scripts/desktop-package.mjs --os macos --variant finance",
    "desktop:package:windows:finance": "node scripts/desktop-package.mjs --os windows --variant finance",
    "desktop:package:macos:finance:sign": "node scripts/desktop-package.mjs --os macos --variant finance --sign",
    "desktop:package:windows:finance:sign": "node scripts/desktop-package.mjs --os windows --variant finance --sign",
    "desktop:package": "node scripts/desktop-package.mjs"
  },
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@tauri-apps/cli": "^2.10.0",
    "@types/canvas-confetti": "^1.9.0",
    "@types/d3": "^7.4.3",
    "@types/maplibre-gl": "^1.13.2",
    "@types/papaparse": "^5.5.2",
    "@types/topojson-client": "^3.1.5",
    "@types/topojson-specification": "^1.0.5",
    "esbuild": "^0.27.3",
    "cross-env": "^7.0.3",
    "markdownlint-cli2": "^0.20.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-pwa": "^1.2.0",
    "ws": "^8.19.0"
  },
  "dependencies": {
    "@deck.gl/aggregation-layers": "^9.2.6",
    "@deck.gl/core": "^9.2.6",
    "@deck.gl/geo-layers": "^9.2.6",
    "@deck.gl/layers": "^9.2.6",
    "@deck.gl/mapbox": "^9.2.6",
    "@sentry/browser": "^10.39.0",
    "@upstash/redis": "^1.36.1",
    "@vercel/analytics": "^1.6.1",
    "@xenova/transformers": "^2.17.2",
    "canvas-confetti": "^1.9.4",
    "convex": "^1.32.0",
    "d3": "^7.9.0",
    "deck.gl": "^9.2.6",
    "fast-xml-parser": "^5.3.7",
    "i18next": "^25.8.10",
    "i18next-browser-languagedetector": "^8.2.1",
    "maplibre-gl": "^5.16.0",
    "onnxruntime-web": "^1.23.2",
    "papaparse": "^5.5.3",
    "posthog-js": "^1.352.0",
    "topojson-client": "^3.1.0",
    "youtubei.js": "^16.0.1"
  },
  "overrides": {
    "fast-xml-parser": "^5.3.7"
  }
}
